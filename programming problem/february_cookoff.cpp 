#include<bits/stdc++.h>
using namespace std;

int main()
{
int t;
cin>>t;
for(int i=0;i<t;i++)
{
int n;
cin>>n;
for(int j=0;j<n;j++)
{	
for(int k=0;k<n;k++)
{
cin>>a[j][k];
if(k<n/2 && a[j][k]==1)
{count1++;}
if(k>=n/2 && a[j][k]==1)
{count2++;}
if(abs(count1-count2)>0)
{ reverse(a[j], a[j] + n);
	x = count1-count2;
}
 

}
		
		
		return 0;
}
