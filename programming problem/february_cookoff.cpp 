#include<bits/stdc++.h>
using namespace std;

int main()
{
int t;
cin>>t;
long int a[6];
a[0] = 250000;
a[1] = 500000;
a[2] = 750000;
a[3] = 1000000;
a[4] = 1250000;
a[5] = 1500000;


long long int x;
for(int j=0;j<t;j++)
{
cin>>x;
long long int sum =0;
if(x<=a[0])
{cout<<x<<endl;}
else if(x>a[0])
{
if(x>a[0] && x<=a[1])
{sum = (x-a[0])*0.05;}
else if(x>a[1] && x<=a[2])
{sum  += (a[1]-a[0])*0.05 + (x-a[1])*0.1;
	}
else if(x>a[2] && x<=a[3])
{sum += (a[1]-a[0])*0.05 + (a[2]-a[1])*0.1 + (x-a[2])*0.15;}
else if(x>a[3] && x<=a[4])
{sum += (a[1]-a[0])*0.05 + (a[2]-a[1])*0.1 +(a[3]-a[2])*0.15 + (x - a[3])*0.2;}
else if(x>a[4] && x<=a[5])
{sum += (a[1]-a[0])*0.05 + (a[2]-a[1])*0.1 +(a[3]-a[2])*0.15 + (a[4] - a[3])*0.2 + (x - a[4])*0.25;}
else if(  x>a[5])
{sum += (a[1]-a[0])*0.05 + (a[2]-a[1])*0.1 +(a[3]-a[2])*0.15 +  (a[4] - a[3])*0.2 + (a[5] - a[4])*0.25 + (x-a[5])*0.3;}
x = x-sum;
cout<<x<<endl;
}
}

	
	return 0;
}



#include<bits/stdc++.h>
using namespace std;

int main()
{
int t;
cin>>t;
for(int i=0;i<t;i++)
{
string s;
int n;
cin>>n;
cin>>s;
int a=0,b=0,c=0;
int counta=0,countb=0;
for(int j =0;j<2*n;j++)
{
	if(j%2==0)
	{	counta++;
		if(s[j]=='1')
		{a++;}	
	}
	else if(j%2!=0)
	{countb++;
		if(s[j]=='1')
		{b++;}
	}
	
	if(a-b>n-countb)
	{cout<<j+1<<endl;
	c=1;
		break;
	}
	if(b-a>n-counta)
	{cout<<j+1<<endl;
	c = 1;
		break;
	}	
}
if(c==0)
{cout<<2*n<<endl;}	
}	
	
	return 0;
}


