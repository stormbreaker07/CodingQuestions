

#include<bits/stdc++.h>
using namespace std;

long int maxSubArraySum(vector <int> a, int size) 
{ 
    long int max_so_far = INT_MIN, max_ending_here = 0; 
  
    for (int i = 0; i < size; i++) 
    { 
        max_ending_here = max_ending_here + a[i]; 
        if (max_so_far < max_ending_here) 
            max_so_far = max_ending_here; 
  
        if (max_ending_here < 0) 
            max_ending_here = 0; 
    } 
    return max_so_far; 
} 


int main()
{
int t,n;
cin>>t;
int a;
vector<int> v;
for(int i=0;i<t;i++)
{
cin>>n;
for(int j=0;j<n;j++)
{
cin>>a;
v.push_back(a);
}
long int sum = maxSubArraySum(v,v.size());
cout<<sum<<" ";
for(int j=0;j<n-1;j++)
{
v.push_back(v[j]);
v[j]=0;
long int sum = maxSubArraySum(v,v.size());	
cout<<sum<<" ";
}

}	
	
	
	return 0;
}
